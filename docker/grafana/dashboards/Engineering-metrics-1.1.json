            "alias": "Calendar Duration",
            "binary": {
              "left": {
                "matcher": {
                  "id": "byName",
                  "options": "Duration"
                }
              },
              "operator": "*",
              "right": {
                "fixed": "1.4"
              }
            },
            "mode": "binary",
            "reduce": {
              "reducer": "sum"
            },
            "replaceFields": false
          }
        },
        {
          "id": "groupBy",
          "options": {
            "fields": {
              "Calendar Duration": {
                "aggregations": [
                  "p85"
                ],
                "operation": "aggregate"
              },
              "Duration": {
                "aggregations": [
                  "p85"
                ]
              },
              "Duration * 1.4": {
                "aggregations": [
                  "median"
                ],
                "operation": "aggregate"
              },
              "Duration / 5": {
                "aggregations": [
                  "mean",
                  "median"
                ],
                "operation": "aggregate"
              },
              "Sprint": {
                "aggregations": [],
                "operation": "groupby"
              },
              "round(Duration * 1.4)": {
                "aggregations": [
                  "median",
                  "mean"
                ],
                "operation": "aggregate"
              }
            }
